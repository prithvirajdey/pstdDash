    "bootstrap-loader": "^1.0.8",
    "chart.js": "^1.1.1",
    "chartist": "^0.9.7",
    "core-js": "^2.2.2",
    "easy-pie-chart": "^2.1.7",
    "extract-text-webpack-plugin": "^1.0.1",
    "font-awesome": "^4.6.1",
    "font-awesome-sass-loader": "^1.0.1",
    "fullcalendar": "^2.7.2",
    "google-maps": "^3.2.1",
    "jquery": "^2.2.3",
    "jquery-slimscroll": "^1.3.6",
    "leaflet": "^0.7.7",
    "leaflet-map": "^0.2.1",
